#macro XOR %register, %register, %register, %register
#comment begin xor %0, %1, %2, %3: %__macro_id, %__macro_no, %__macro_head
#comment %0 xor %1, %2 & %3 are temporary
CPY %0, %2
CPY %1, %3
NOT %2
NOT %3
AND %0, %3
AND %2, %1
OR %0, %2
#comment end xor
#endmacro


#macro NAND %register, %register
#comment begin nand
AND %0, %1
NOT %0
#comment end nand
#endmacro


#macro NOR %register, %register
#comment begin nor
OR %0, %1
NOT %0
#comment end nor
#endmacro